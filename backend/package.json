{
  "name": "Vietbaguette-backend",
  "version": "0.1.1",
  "type": "module",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "prisma generate && nodemon --exec tsx src/index.ts",
    "build": "prisma generate && tsc -p tsconfig.production.json",
    "start": "node dist/index.js",
    "seed:menu": "tsx scripts/seedMenu.ts",
    "seed:admin": "tsx scripts/seedAdmin.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "node scripts/prisma-migrate.js dev"
  },
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "jsonwebtoken": "^9.0.2",
    "socket.io": "^4.8.1",
    "zod": "^3.25.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.19",
    "nodemon": "^3.1.10",
    "prisma": "^6.8.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "vercel": {
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.ts"
      }
    ]
  }
}
